[color]
	branch = auto
	diff = auto
	status = auto
[push]
	default = simple
[help]
	autocorrect = 15
[pretty]
	me = "%Cgreen%h%Creset - %C(230)%s%Creset"

# name, email, and excludesfile are in the local file.
[include]
	path = ~/.gitconfig_local

[alias]
	sync="! git fetch -p && git branch -vv | awk '/: gone]/{print $1}' | xargs git branch -D"
